{
    "editor.fontSize": 13,
    "editor.fontFamily": "JetBrains Mono",
    "editor.bracketPairColorization.enabled": false,
    "editor.tokenColorCustomizations": {
        "comments": "#999999",
        "keywords": "#f8f8f2",
        "functions": "#f8f8f2",
        "types": "#f8f8f2",
        "strings": "#f8f8f2",
        "numbers": "#f8f8f2",
        "textMateRules": [
            {
                "scope": "constant.language.lean4",
                "settings": {
                    "foreground": "#f8f8f2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable",
                "settings": {
                    "foreground": "#aabbff",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable.other.property",
                "settings": {
                    "foreground": "#f8f8f2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "storage.type.lean4",
                "settings": {
                    "foreground": "#f8f8f2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "keyword",
                "settings": {
                    "foreground": "#ffaaaa",
                    "fontStyle": ""
                }
            },
            {
                "scope": "markup.inline.raw.string.markdown",
                "settings": {
                    "foreground": "#339900",
                    "fontStyle": ""
                }
            },
            {
                "scope": "entity.name.function.lean4",
                "settings": {
                    "foreground": "#f8f8f2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "string.other.link.title.markdown",
                "settings": {
                    "foreground": "#999999",
                    "fontStyle": ""
                }
            },
        ]
    },
    "highlight.regexes": {
        "(∧|∨|→|↔|∀|∃|:=)": [
            {
                "color": "#ffaaaa"
            }
        ],
        "(sorry)": [
            {
                "color": "#000000",
                "backgroundColor": "#ff0000",
            }
        ]
    },
}
