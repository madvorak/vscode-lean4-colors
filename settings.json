{
    "editor.fontSize": 13,
    "editor.fontFamily": "JetBrains Mono",
    "editor.bracketPairColorization.enabled": false,
    "editor.tokenColorCustomizations": {
        "comments": "#999999",
        "keywords": "#e8e8e2",
        "functions": "#e8e8e2",
        "types": "#e8e8e2",
        "strings": "#e8e8e2",
        "numbers": "#e8e8e2",
        "textMateRules": [
            {
                "scope": "constant.language.lean4",
                "settings": {
                    "foreground": "#e8e8e2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "constant.character.escape.lean4",
                "settings": {
                    "foreground": "#e8e8e2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable",
                "settings": {
                    "foreground": "#aaccff",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable.other.property",
                "settings": {
                    "foreground": "#e8e8e2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "storage.type.lean4",
                "settings": {
                    "foreground": "#e8e8e2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "keyword",
                "settings": {
                    "foreground": "#ffaaaa",
                    "fontStyle": ""
                }
            },
            {
                "scope": "markup.inline.raw.string.markdown",
                "settings": {
                    "foreground": "#339900",
                    "fontStyle": ""
                }
            },
            {
                "scope": "entity.name.function.lean4",
                "settings": {
                    "foreground": "#e8e8e2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "string.other.link.title.markdown",
                "settings": {
                    "foreground": "#999999",
                    "fontStyle": ""
                }
            },
            {
                "scope": "source.lean4",
                "settings": {
                    "foreground": "#e8e8e2",
                    "fontStyle": ""
                }
            },
        ]
    },
    "highlight.regexes": {
        "(∧|∨|→|↔|∀|∃|:=|=>)": [
            {
                "color": "#ffaaaa"
            }
        ],
        "(sorry)": [
            {
                "color": "#000000",
                "backgroundColor": "#ff0000",
            }
        ]
    },
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Monokai",
    "diffEditor.ignoreTrimWhitespace": false
}
