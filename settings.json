{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.olean": true
    },
    "git.openRepositoryInParentFolders": "never",
    "editor.fontSize": 13,
    "editor.fontFamily": "Jetbrains Mono",
    "editor.bracketPairColorization.enabled": false,
    "editor.tokenColorCustomizations": {
        "keywords": "#dddddd",
        "functions": "#dddddd",
        "types": "#dddddd",
        "strings": "#dddddd",
        "numbers": "#dddddd",
        "comments": "#999999",
        "textMateRules": [
            {
                "scope": "keyword",
                "settings": {
                    "foreground": "#ffaaaa",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable",
                "settings": {
                    "foreground": "#aaccff",
                    "fontStyle": ""
                }
            },
            {
                "scope": "source.lean4",
                "settings": {
                    "foreground": "#dddddd",
                    "fontStyle": ""
                }
            },
            {
                "scope": "constant.language.lean4",
                "settings": {
                    "foreground": "#dddddd",
                    "fontStyle": ""
                }
            },
            {
                "scope": "constant.character.escape.lean4",
                "settings": {
                    "foreground": "#dddddd",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable.other.property",
                "settings": {
                    "foreground": "#dddddd",
                    "fontStyle": ""
                }
            },
            {
                "scope": "storage.type.lean4",
                "settings": {
                    "foreground": "#dddddd",
                    "fontStyle": ""
                }
            },
            {
                "scope": "entity.name.function.lean4",
                "settings": {
                    "foreground": "#dddddd",
                    "fontStyle": ""
                }
            },
            {
                "scope": "markup.inline.raw.string.markdown",
                "settings": {
                    "foreground": "#339900",
                    "fontStyle": ""
                }
            },
            {
                "scope": "markup.italic.markdown",
                "settings": {
                    "foreground": "#339900",
                    "fontStyle": ""
                }
            },
            {
                "scope": "entity.name.section.markdown",
                "settings": {
                    "foreground": "#339900",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "markup.list.unnumbered.markdown",
                "settings": {
                    "foreground": "#999999",
                    "fontStyle": ""
                }
            },
            {
                "scope": "string.other.link.title.markdown",
                "settings": {
                    "foreground": "#999999",
                    "fontStyle": ""
                }
            },
            {
                "scope": "punctuation.definition.heading.markdown",
                "settings": {
                    "foreground": "#999999",
                    "fontStyle": ""
                }
            },
            {
                "scope": "markup.list.unnumbered.markdown",
                "settings": {
                    "foreground": "#999999",
                    "fontStyle": ""
                }
            },
            {
                "scope": "punctuation.definition.list.begin.markdown",
                "settings": {
                    "foreground": "#999999",
                    "fontStyle": ""
                }
            },
        ]
    },
    "highlight.regexes": {
        "(∧|∨|↔|∀|∃| →|:=|=>)": [
            {
                "color": "#ffaaaa"
            }
        ],
        "(sorry)": [
            {
                "color": "#000000",
                "backgroundColor": "#ff0000",
            }
        ]
    },
    "lean4.infoview.style": ".goal-hyp {color: #aaccff !important; font-weight: normal !important;} .goal-vdash {color: #ffaaaa !important;} .goal-goals {color: #ffaaaa !important;} .goal-case {color: #dddddd !important;}",
    "workbench.colorTheme": "Monokai",
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.accessibilitySupport": "off",
    "editor.stickyScroll.enabled": false
}
